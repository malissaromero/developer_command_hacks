<%= form_for(@post) do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>

  <%= f.label :command %>
  <%= f.text_field :command %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit %>

<% end %>
