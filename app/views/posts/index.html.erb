<h1>#Dev {Command Hacks}</h1>
<h4> <%= link_to "Home", posts_path %>
     <%= link_to "Create Command", new_post_path %>
     <%= link_to "My Commands", user_posts_path(:user_id) %>
</h4>

<% @posts.each do |post| %>
  <div>
    <p> <%= link_to "#{post.title}", post_path(post) %></p>
    <p> <%= post.command %> </p>
    <p> <%= post.description %> </p>
  </div>
<% end %>

